{
    "name": "heyi",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "cache-control": "public, max-age=3600, s-maxage=86400"
            }
        }
    ],
    "github": {
        "silent": true
    },
    "cleanUrls": true,
    "trailingSlash": false
}