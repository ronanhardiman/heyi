{
    "name": "静态网站",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "github": {
        "silent": true
    }
}