{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/public/**/*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}